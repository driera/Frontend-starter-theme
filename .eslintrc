{
    "globals": {
        "$": true
    },
    "env": {
        "es6": true,
        "browser": true,
        "jquery": true,
        "commonjs": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
        },
    },
    "rules": {
        "array-bracket-spacing": 1,
        "arrow-body-style": [1, "as-needed"],
        "arrow-parens": [2, "as-needed", {
            "requireForBlockBody": true
        }],
        "arrow-spacing": 2,
        "brace-style": 1,
        "curly": [2, "all"],
        "comma-dangle": 1,
        "comma-spacing": 1,
        "comma-style": 2,
        "consistent-this": [2, "self"],
        "constructor-super": 1,
        "eol-last": 1,
        "func-name-matching": 2,
        "indent": [2, 4],
        "implicit-arrow-linebreak": [2, "beside"],
        "key-spacing": 2,
        "lines-between-class-members": [1, "always", {
            "exceptAfterSingleLine": true
        }],
        "new-cap": 2,
        "new-parens": 2,
        "no-alert": 2,
        "no-bitwise": 2,
        "no-case-declarations": 1,
        "no-class-assign": 1,
        "no-compare-neg-zero": 2,
        "no-console": 2,
        "no-cond-assign": 1,
        "no-const-assign": 2,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-dupe-class-members": 1,
        "no-duplicate-case": 2,
        "no-duplicate-imports": 1,
        "no-eq-null": 2,
        "no-empty": 1,
        "no-empty-character-class": 1,
        "no-empty-pattern": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 0,
        "no-extra-semi": 1,
        "no-fallthrough": 1,
        "no-func-assign": 1,
        "no-global-assign": 1,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 1,
        "no-mixed-operators": 1,
        "no-mixed-spaces-and-tabs": 2,
        "no-multi-assign": 1,
        "no-multiple-empty-lines": 1,
        "no-new-object": 1,
        "no-new-symbol": 1,
        "no-obj-calls": 1,
        "no-octal": 1,
        "no-plusplus": 1,
        "no-redeclare": 1,
        "no-regex-spaces": 1,
        "no-self-assign": 1,
        "no-sparse-arrays": 2,
        "no-trailing-spaces": 1,
        "no-this-before-super": 1,
        "no-undef": 1,
        "no-unexpected-multiline": 1,
        "no-unreachable": 2,
        "no-unsafe-finally": 1,
        "no-unsafe-negation": 1,
        "no-unused-labels": 1,
        "no-unused-vars": 1,
        "no-unneeded-ternary": 1,
        "no-useless-concat": 1,
        "no-useless-constructor": 2,
        "no-useless-escape": 1,
        "no-var": 1,
        "object-curly-newline": [2, {
            "consistent": true
        }],
        "object-curly-spacing": 1,
        "one-var": [1, "never"],
        "operator-assignment": 1,
        "quote-props": [1, "consistent"],
        "quotes": [1, "single", {
            "avoidEscape": true,
            "allowTemplateLiterals": true
        }],
        "require-yield": 1,
        "semi": [1, "always", {
            "omitLastInOneLineBlock": true
        }],
        "semi-spacing": 2,
        "semi-style": [2, "last"],
        "space-before-blocks": 1,
        "space-before-function-paren": [1, "never"],
        "space-in-parens": 1,
        "space-unary-ops": 1,
        "switch-colon-spacing": 1,
        "template-curly-spacing": 1,
        "use-isnan": 1,
        "valid-typeof": 2,
        "wrap-regex": 1
    },
}
